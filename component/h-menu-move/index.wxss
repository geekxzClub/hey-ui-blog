/* components/menu/menu.wxss */
movable-view {
    pointer-events: auto; 
}

movable-area {
    pointer-events: none;
}
.menuarea{
  position: fixed;
  left: .0rpx;
  right: .0rpx;
  top: .0rpx;
  bottom: .0rpx;
  height: 100%;
  width: 100%;
}
.menu_box{
  position: fixed;
  bottom: 150rpx;
  right: 30rpx;
  /* background-color: red; */
  text-align: center;
}
.menu_main{
  top: 10rpx;
  width: 100rpx;
  height: 100rpx;
  margin-top: 10rpx;
  background-color: rgb(214, 216, 221);
  border-radius: 200rpx;
}
.menu_img{
  margin-top: 10rpx;
  width: 40rpx;
  height: 40rpx;
}
.menu_title{
  display: block;
  width: 100%;
  text-align: center;
  font-size: 22rpx;
}
