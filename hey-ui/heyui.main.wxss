/* ==================
    阴影 Shadow
 ==================== */
 /* -- 实线 -- */
.h-border,
.h-border-top,
.h-border-right,
.h-border-bottom,
.h-border-left,
.h-borders,
.h-borders-top,
.h-borders-right,
.h-borders-bottom,
.h-borders-left,
.h-dashed,
.h-dashed-top,
.h-dashed-right,
.h-dashed-bottom,
.h-dashed-left {
	position: relative;
}
.h-border::after,
.h-border-top::after,
.h-border-right::after,
.h-border-bottom::after,
.h-border-left::after,
.h-borders::after,
.h-borders-top::after,
.h-borders-right::after,
.h-borders-bottom::after,
.h-borders-left::after,
.h-dashed::after,
.h-dashed-top::after,
.h-dashed-right::after,
.h-dashed-bottom::after,
.h-dashed-left::after {
	content: " ";
	width: 200%;
	height: 200%;
	position: absolute;
	top: 0;
	left: 0;
	border-radius: inherit;
	transform: scale(0.5);
	transform-origin: 0 0;
	pointer-events: none;
	box-sizing: border-box;
}
.h-border::after {
	border: 2rpx solid rgba(0, 0, 0, 0.2);
}
.h-border-top::after {
	border-top: 2rpx solid rgba(0, 0, 0, 0.2);
}
.h-border-right::after {
	border-right: 2rpx solid rgba(0, 0, 0, 0.2);
}
.h-border-bottom::after {
	border-bottom: 2rpx solid rgba(0, 0, 0, 0.2);
}
.h-border-left::after {
	border-left: 2rpx solid rgba(0, 0, 0, 0.2);
}
.h-borders::after {
	border: 8rpx solid #eee;
}
.h-borders-top::after {
	border-top: 8rpx solid #eee;
}
.h-borders-right::after {
	border-right: 8rpx solid #eee;
}
.h-borders-bottom::after {
	border-bottom: 8rpx solid #eee;
}
.h-borders-left::after {
	border-left: 8rpx solid #eee;
}
/* -- 虚线 -- */
.h-dashed::after {
	border: 2rpx dashed #ddd;
}
.h-dashed-top::after {
	border-top: 2rpx dashed #ddd;
}
.h-dashed-right::after {
	border-right: 2rpx dashed #ddd;
}
.h-dashed-bottom::after {
	border-bottom: 1rpx dashed #ddd;
}
.h-dashed-left::after {
	border-left: 1rpx dashed #ddd;
}
/* 阴影 */
 .h-shadow{
	box-shadow: 0 0rpx 1rpx rgba(0, 0, 0, 0.1);
}
.h-shadow-lg {
	box-shadow: 0 0 6rpx rgba(0, 0, 0, 0.1);
}
.h-shadow-warp {
	position: relative;
	box-shadow: 0 0 10rpx rgba(0, 0, 0, 0.1);
}
.h-shadow-warp:before,
.h-shadow-warp:after {
	position: absolute;
	content: "";
	top: 20rpx;
	bottom: 30rpx;
	left: 20rpx;
	width: 50%;
	box-shadow: 0 30rpx 20rpx rgba(0, 0, 0, 0.2);
	transform: rotate(-3deg);
	z-index: -1;
}
.h-shadow-warp:after {
	right: 20rpx;
	left: auto;
	transform: rotate(3deg);
}
.h-shadow-img {
	position: relative;
}
.h-shadow-img::before {
	content: "";
	display: block;
	background: inherit;
	filter: blur(10rpx);
	position: absolute;
	width: 100%;
	height: 100%;
	top: 10rpx;
	left: 10rpx;
	z-index: -1;
	opacity: 0.4;
	transform-origin: 0 0;
	border-radius: inherit;
	transform: scale(1, 1);
}
.h-shadow[class*="h-bg-red"] {
	box-shadow: var(--ShadowSize) var(--redShadow);
}
.h-shadow[class*="h-bg-orange"] {
	box-shadow: var(--ShadowSize) var(--orangeShadow);
}
.h-shadow[class*="h-bg-yellow"] {
	box-shadow: var(--ShadowSize) var(--yellowShadow);
}
.h-shadow[class*="h-bg-olive"] {
	box-shadow: var(--ShadowSize) var(--oliveShadow);
}
.h-shadow[class*="h-bg-green"] {
	box-shadow: var(--ShadowSize) var(--greenShadow);
}
.h-shadow[class*="h-bg-cyan"] {
	box-shadow: var(--ShadowSize) var(--cyanShadow);
}
.h-shadow[class*="h-bg-blue"] {
	box-shadow: var(--ShadowSize) var(--blueShadow);
}
.h-shadow[class*="h-bg-purple"] {
	box-shadow: var(--ShadowSize) var(--purpleShadow);
}
.h-shadow[class*="-mauve"] {
	box-shadow: var(--ShadowSize) var(--mauveShadow);
}
.h-shadow[class*="h-bg-pink"] {
	box-shadow: var(--ShadowSize) var(--pinkShadow);
}
.h-shadow[class*="h-bg-brown"] {
	box-shadow: var(--ShadowSize) var(--brownShadow);
}
.h-shadow[class*="h-bg-grey"] {
	box-shadow: var(--ShadowSize) var(--greyShadow);
}
.h-shadow[class*="h-bg-gray"] {
	box-shadow: var(--ShadowSize) var(--grayShadow);
}
.h-shadow[class*="h-bg-black"] {
	box-shadow: var(--ShadowSize) var(--blackShadow);
}
.h-shadow[class*="h-bg-white"] {
	box-shadow: var(--ShadowSize) var(--blackShadow);
}



/* button start */
.h-btn {
	position: relative;
	border: 0rpx;
	display: inline-flex;
	align-items: center;
	justify-content: center;
	box-sizing: border-box;
	padding: 0 30rpx;
	font-size: 28rpx;
	height: 64rpx;
	line-height: 1;
	text-align: center;
	text-decoration: none;
	overflow: visible;
	margin-left: initial;
	transform: translate(0rpx, 0rpx);
	margin-right: initial;
}
.h-btn::after {
	display: none;
}
.h-success-hover {
  background: var(--success) !important;
  color: #e5e5e5 !important;
}
.h-error-hover {
  background: var(--error) !important;
  color: #e5e5e5 !important;
}
.h-warning-hover {
  background: var(--warning) !important;
  color: #e5e5e5 !important;
}
.h-primary-hover {
  background: var(--primary) !important;
  color: #e5e5e5 !important;
}
.h-black-hover {
  background: var(--black) !important;
  color: #e5e5e5 !important;
}
.h-teanslucent-hover {
  background: var(--teanslucent) !important;
  color: #e5e5e5 !important;
}
.h-btn:not([class*="h-bg-"]) {
	background-color: #f0f0f0;
}
.h-btn[class*="h-line"] {
	background-color: transparent;
}
.h-btn[class*="h-line"]::after {
	content: " ";
	display: block;
	width: 200%;
	height: 200%;
	position: absolute;
	top: 0;
	left: 0;
	border: 1rpx solid currentColor;
	transform: scale(0.5);
	transform-origin: 0 0;
	box-sizing: border-box;
	border-radius: 12rpx;
	z-index: 1;
	pointer-events: none;
}
.h-btn.h-btn-sm {
	padding: 0 20rpx;
	font-size: 20rpx;
	height: 48rpx;
}
.h-btn.h-btn-lg {
	padding: 0 40rpx;
	font-size: 32rpx;
	height: 80rpx;
}
.h-btn.icon.icon-sm {
	width: 48rpx;
	height: 48rpx;
}
.h-btn.icon {
	width: 64rpx;
	height: 64rpx;
	border-radius: 500rpx;
	padding: 0;
}
button.icon.icon-lg {
	width: 80rpx;
	height: 80rpx;
}
.h-btn.button-hover {
	transform: translate(2rpx, 2rpx);
}
.h-btn.h-btn-block {
	display: flex;
  height: 100rpx;
}
.h-btn[disabled] {
	opacity: 0.6;
	color: var(--white);
}
/* button end */

/* ==================
      标签徽章
 ==================== */

.h-tag {
	font-size: 24rpx;
	vertical-align: middle;
	position: relative;
	display: inline-flex;
	align-items: center;
	justify-content: center;
	box-sizing: border-box;
	padding: 0rpx 16rpx;
	height: 48rpx;
	font-family: Helvetica Neue, Helvetica, sans-serif;
	white-space: nowrap;
}
.h-tag:not([class*="h-bg"]):not([class*="h-line"]) {
	background-color: var(--ghostWhite);
}
.h-tag[class*="h-line-"]::after {
	content: " ";
	width: 200%;
	height: 200%;
	position: absolute;
	top: 0;
	left: 0;
	border: 1rpx solid currentColor;
	transform: scale(0.5);
	transform-origin: 0 0;
	box-sizing: border-box;
	border-radius: inherit;
	z-index: 1;
	pointer-events: none;
}
.h-tag[class*="h-line-"]::after {
	border-radius: 0;
}
.h-tag+.h-tag {
	margin-left: 10rpx;
}
.h-tag.h-tag-sm {
	/* font-size: 20rpx; */
	padding: 0rpx 12rpx;
	height: 32rpx;
}
.h-tag.h-tag-lg {
	/* font-size: 32rpx; */
	padding: 8rpx 22rpx;
	height: 60rpx;
}
.h-capsule {
	display: inline-flex;
	vertical-align: middle;
}
.h-capsule+.h-capsule {
	margin-left: 15rpx;
}
.h-capsule .h-tag {
